<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Gender</th>
    <th>Country</th>
    <th>City</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.name %></td>
    <td><%= profile.age %></td>
    <td><%= profile.gender %></td>
    <td><%= profile.country %></td>
    <td><%= profile.city %></td>
    <td><%= link_to 'Show', profile %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
